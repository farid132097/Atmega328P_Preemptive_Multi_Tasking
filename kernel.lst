 227               	.macro  KER_WDT_INIT                                      ;1.13uS @8MHz           (  9 clocks) 
 228               	        WDR                                               ;reset wdt              (  1 clock ) 
 229               			LDS   R18                , SRWDTCSR               ;copy WDTCSR            (  2 clocks) 
