 781               	;;============================sleep timeout management starting=============================;; 
 782               	;used registers          : R18, R19, R20, R24, R30(ZL), R31(ZH)                                
 783               	;arg registers           : R24 (SCH_MODE_HANDLER/SCH_MODE_THREAD)                              
 784               	;return registers        : R24 (READY/BLOCKED/EXECUTING/SUSPENDED/CONS_LAT)                    
 785               	;unsafe access registers : R18, R19, R20, R24, R30(ZL), R31(ZH)                                
 786               	.macro  KER_SLP_TIME_MGNT                                 ;total 6.50uS @8MHz     ( 52 clocks) 
 787               			LDI   R30                , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 788               			LDI   R31                , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 789               			KER_CALC_ADDR_OFF_WORD                            ;calc offset            (  6 clocks) 
 790               			;fetch current value from ram, if val=0, skip decrement                                
 791               	        LDD   R18                , Z+0                    ;load val low byte      (  2 clocks) 
 792               			LDD   R19                , Z+1                    ;load val high byte     (  2 clocks) 
 793               			MOV   R20                , R18                    ;copy                   (  1 clock ) 
 794               			OR    R20                , R19                    ;or high & low bytes    (  1 clock ) 
 795               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 796               	        CPI   R24                , SCH_MODE_THREAD        ;if arg=1, thread mode  (  1 clock ) 
 797               			BREQ  _VAL_NOT_NULL\@                             ;no need to dec val     (  2 clocks) 
 798               			;R19:R18 contains 16 bit sleep timer val, decrease val by 1                            
 799               			LDI   R20                , 0x01                   ;set val 1              (  1 clock ) 
 800               	        SUB   R18                , R20                    ;subtract low byte      (  1 clock ) 
 801               			LDI   R20                , 0x00                   ;clear                  (  1 clock ) 
 802               			SBC   R19                , R20                    ;subtract carry if any  (  1 clock ) 
 803               			;store new value                                                                       
 804               			STD   Z+0                , R18                    ;store low byte         (  2 clocks) 
 805               			STD   Z+1                , R19                    ;store low byte         (  2 clocks) 
 806               			MOV   R20                , R18                    ;copy                   (  1 clock ) 
 807               			OR    R20                , R19                    ;or high & low bytes    (  1 clock ) 
 808               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 809               		_VAL_NULL\@:                                                                               
 810               		    ;find ram address for status                                                           
 811               		    LDI   R30                , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 812               			LDI   R31                , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 813               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 814               			;update flag as task is ready                                                          
 815               			LDI   R24                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 816               			ST    Z                  , R24                    ;update flag            (  2 clocks) 
 817               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
