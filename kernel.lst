 413               			LDS   R17                , KerBase+OFF_NTSK       ;load ntask             (  2 clocks) 
 414               		_MOD_CONT\@:                                          ;use local label
 415               		    CP    R16                , R17                    ;compare R17 with R16   (  1 clock ) 
 416               			BRLO  _MOD_DONE\@                                 ;if R16<R17, done       (  2 clocks) 
 417               			SUB   R16                , R17                    ;subtract R17 from R16  (  1 clock ) 
 418               			RJMP  _MOD_CONT\@                                 ;go to loop start       (  2 clocks) 
 419               		_MOD_DONE\@:                                          ;use local label
 420               		    STS   KerBase+OFF_TID    , R16                    ;save task_id%ntask     (  2 clocks) 
