 301               	.macro  KER_SAVE_CURR_TASK_SP                             ;total 1.88uS @8MHz     ( 15 clocks) 
 302               			IN    R16                , IOSPL                  ;fetch SPL0             (  1 clock ) 
 303               			IN    R17                , IOSPH                  ;fetch SPH0             (  1 clock ) 
 304               			LDS   R22                , KerBase+OFB_TID        ;fetch task_id, ret var (  2 clocks) 
 305               			MOV   R18                , R22                    ;copy                   (  1 clock ) 
 306               			LSL   R18                                         ;left shift to multiply (  1 clock ) 
 307               			LDI   ZL                 , lo8(KerPSP)            ;fetch base pos low     (  1 clock ) 
 308               			LDI   ZH                 , hi8(KerPSP)            ;fetch base pos high    (  1 clock ) 
 309               			ADD   ZL                 , R18                    ;add offset to array    (  1 clock ) 
 310               			LDI   R18                , 0x00                   ;clear for carry prop   (  1 clock ) 
 311               			ADC   ZH                 , R18                    ;add carry, R1 zero reg (  1 clock ) 
 312               			ST    Z+                 , R16                    ;store SPL at ZP        (  2 clocks) 
 313               			ST    z                  , R17                    ;store SPH ZP+1         (  2 clocks) 
 314               	.endm
