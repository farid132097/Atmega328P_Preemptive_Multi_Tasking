 358               			LDI   R18                , 0x45                   ;WDIE, WDPS1, WDPS2     (  1 clock ) 
 359               			#elif defined(KER_WDT_TICK_1000MS)                                                     
 360               			LDI   R18                , 0x46                   ;WDIE, WDPS1, WDPS2     (  1 clock ) 
 361               			#else                                                                                  
