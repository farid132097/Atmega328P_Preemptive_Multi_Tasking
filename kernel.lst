 506               	.macro  KER_RUN_SCHEDULER                                 ;total 4.88uS @8MHz     ( 39 clocks) 
 507               			LDI   R26                , 0xFF                   ;min priority val       (  1 clock ) 
 508               			LDI   R27                , 0x00                   ;holds high prio tak_id (  1 clock ) 
 509               			LDS   R23                , KerBase+OFB_NTSK       ;load ntask             (  2 clocks) 
 510               			MOV   R28                , R23                    ;copy ntask             (  1 clock ) 
 511               	
 512               		_LOOP\@:
 513               			DEC   R28                                         ;start from ntask-1     (  1 clock ) 
 514               			MOV   R22                , R28                    ;copy task_id, arg1     (  1 clock ) 
 515               			KER_DEC_SLP_TIMEOUT                               ;update return vars     ( 49 clocks) 
 516               	        CPI   R24                , TASK_READY             ;check if task is ready (  1 clock ) 
 517               	        BRNE  _LOOP_CNT\@                                 ;if task not ready, skip(  2 clocks) 
 518               			CP    R25                , R26                    ;check priority         (  1 clock ) 
 519               			BRSH  _LOOP_CNT\@
 520               		    MOV   R26                , R25                    ;copy new low priority  (  1 clock ) 
 521               		    MOV   R27                , R28                    ;copy lowest priotask_id(  1 clock ) 
 522               	        
 523               	    _LOOP_CNT\@:
 524               			TST   R28                                         ;check if 0             (  1 clock ) 
 525               	        BRNE  _LOOP\@                                     ;if task_id!=0, again   (  2 clocks) 
 526               	        STS   KerBase+OFB_PTID   , R27                    ;store high prio tak_id (  2 clocks) 
 527               			;MOV   R22                , R27                    ;copy task_id, arg1     (  1 clock ) 
 528               	.endm
