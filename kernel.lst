 546               	;;================================run scheduler starting====================================;; 
 547               	;used registers          : R16, R24, R30, R31, returns R24 as current task priority            
 548               	;unsafe access registers : None       
 549               	.macro  KER_RUN_SCHEDULER                                 ;total 4.88uS @8MHz     ( 39 clocks) 
 550               			LDI    R16                , 0xFF                  ;set 0xff               (  1 clock ) 
 551               			STS    KerBase+OFB_LPR    , R16                   ;lowest priority        (  2 clocks) 
 552               			LDI    R16                , 0x00                  ;start from 0           (  1 clock ) 
 553               			STS    KerBase+OFB_PTID   , R16                   ;highest prio tid=0     (  2 clocks) 
 554               		_LOOP\@:                                                                                   
 555               		    ;store task id to run from KER_DEC_SLP_TIMEOUT                                         
 556               			STS    KerBase+OFB_TID    , R16                   ;store task id          (  2 clocks) 
 557               	        ;sleep time decrement, update ready/blocked status                                     
 558               			LDI    R24                , 0x00                  ;isr mode, dec slp val  (  1 clock ) 
 559               			KER_DEC_SLP_TIMEOUT                               ;update return vars     ( 49 clocks) 
 560               	        CPI    R24                , TASK_READY            ;compare                (  1 clock ) 
 561               	        BRNE   _NEXT_TASK\@                               ;skip if task not ready (  2 clocks) 
 562               	        ;calculate current task priority
 563               			KER_CURR_TASK_PRIO                                ;get task prio ->R24    (  9 clocks) 
 564               	        ;compare current task priority with lowest priority so far                             
 565               			LDS    R16                , KerBase+OFB_LPR       ;load lowest priority   (  2 clocks) 
 566               			CP     R24                , R16                   ;compare                (  1 clock ) 
 567               			BRSH   _NEXT_TASK\@                               ;if prio>=lowest prio   (  2 clocks) 
 568               			;found new lowest priority                                                             
 569               			STS    KerBase+OFB_LPR    , R24                   ;save lowest priority   (  2 clocks) 
 570               			LDS    R16                , KerBase+OFB_TID       ;load task id           (  2 clocks) 
 571               			STS    KerBase+OFB_PTID   , R16                   ;save lowest priority   (  2 clocks) 
 572               	
 573               	    _NEXT_TASK\@:
 574               		    LDS    R16                , KerBase+OFB_TID       ;load task id           (  2 clocks) 
 575               			INC    R16                                        ;increment by 1         (  1 clock ) 
 576               			LDS    R17                , KerBase+OFB_NTSK      ;load ntask             (  2 clocks) 
 577               			CP     R16                , R17                   ;compare with ntask     (  2 clocks) 
 578               			BRLO   _LOOP\@                                    ;if task_id<ntask       (  2 clocks) 
 579               	        LDS    R16                , KerBase+OFB_PTID      ;load high prio task id (  2 clocks) 
 580               			STS    KerBase+OFB_TID    , R16                   ;for test only          (  2 clocks)
 581               	.endm                                                                                          
