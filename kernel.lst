 482               	;used registers          : R16, R17, R18, R24, R30, R31, arg R24[0->ISR,1->Thrd], returns R24  
 483               	;unsafe access registers : None                                                                
 484               	.macro  KER_DEC_SLP_TIMEOUT                               ;total 4.75uS @8MHz     ( 38 clocks) 
 485               	        LDS   R16                , KerBase+OFB_TID        ;fetch task_id          (  2 clocks) 
 486               			LSL   R16                                         ;x2                     (  1 clock ) 
 487               			LDI   ZL                 , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 488               			LDI   ZH                 , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 489               			ADD   ZL                 , R16                    ;add offset to array    (  1 clock ) 
 490               			LDI   R16                , 0x00                   ;clear                  (  1 clock ) 
 491               			ADC   ZH                 , R16                    ;add carry if any       (  1 clock ) 
 492               	        
 493               			;fetch current value from ram, if val=0, skip decrement
 494               	        LD    R16                , Z+                     ;load val low byte      (  2 clocks) 
 495               			LD    R17                , Z                      ;load val high byte     (  2 clocks) 
 496               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 497               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 498               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 499               	        
 500               	        ;CPI   R24                , 0x01                   ;if arg=1, thrd mode    (  1 clock ) 
 501               			;BREQ  _VAL_NOT_NULL\@                             ;no need to dec val     (  2 clocks) 
 502               	        
 503               			;R17:R16 contains 16 bit sleep timer val, decrease val by 1
 504               			LDI   R18                , 0x01                   ;set val 1              (  1 clock ) 
 505               	        SUB   R16                , R18                    ;subtract low byte      (  1 clock ) 
 506               			LDI   R18                , 0x00                   ;clear                  (  1 clock ) 
 507               			SBC   R17                , R18                    ;subtract carry if any  (  1 clock ) 
 508               	        
 509               			;store new value
 510               			ST    Z                  , R17                    ;store high byte        (  2 clocks) 
 511               			ST    -Z                 , R16                    ;store low byte         (  2 clocks) 
 512               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 513               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 514               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 515               	
 516               		_VAL_NULL\@:
 517               		    ;find ram address for status
 518               			LDS   R16                , KerBase+OFB_TID        ;fetch task_id          (  2 clocks) 
 519               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 520               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 521               			ADD   ZL                 , R16                    ;add offset to array    (  1 clock ) 
 522               			LDI   R16                , 0x00                   ;clear                  (  1 clock ) 
 523               			ADC   ZH                 , R16                    ;add carry if any       (  1 clock ) 
 524               			
 525               			;update flag as task is ready
 526               			LDI   R16                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 527               			ST    Z                  , R16                    ;update flag            (  2 clocks) 
 528               			LDI   R24                , TASK_READY             ;use as return val      (  1 clock ) 
 529               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
 530               	
 531               	    _VAL_NOT_NULL\@:
 532               		    LDI   R24                , TASK_BLOCK             ;use as return val      (  1 clock ) 
 533               	
 534               	    _EXIT_SLP_TIME\@:
 535               	.endm
