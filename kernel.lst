 475               		 
 476               		_UPDATE_TASK_READY\@:
 477               			;start from ntask-1
 478               			DEC   R16                                         ;start from ntask-1     (  1 clock )
 479               			
 480               			;find ram address for sleep counter
 481               			MOV   R18                , R16                    ;load task_id           (  1 clocks)
 482               			LSL   R18                                         ;x2                     (  1 clock )
 483               			LDI   R30                , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 484               			LDI   R31                , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock )
 485               	        ADD   R30                , R18                    ;add offset to array    (  1 clock ) 
 486               			ADC   R31                , R1                     ;add offset, zero reg   (  1 clock ) 
 487               			
 488               			;fetch current value from ram, if val=0, skip decrement
 489               	        LD    R26                , Z+                     ;load val low byte      (  2 clocks) 
 490               			LD    R27                , Z                      ;load val high byte     (  2 clocks) 
 491               			MOV   R18                , R26                    ;copy                   (  1 clock ) 
 492               			OR    R18                , R27                    ;or high & low bytes    (  1 clock ) 
 493               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 494               			
 495               			;decrease val by 1
 496               			SBIW  R26                , 0x01                   ;decrement 1 from XP    (  2 clocks) 
 497               			ST    Z                  , R27                    ;store low byte         (  2 clocks) 
 498               			ST    -Z                 , R26                    ;store high byte        (  2 clocks) 
 499               			MOV   R18                , R26                    ;copy                   (  1 clock ) 
 500               			OR    R18                , R27                    ;or high & low bytes    (  1 clock ) 
 501               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 502               			JMP   _LOOP\@                                     ;continue until lc=0    (  2 clocks) 
 503               	        
 504               	    _VAL_NULL\@:
 505               		    ;find ram address for status
 506               		    LDI   R30                , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 507               			LDI   R31                , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 508               			ADD   R30                , R16                    ;add offset to array    (  1 clock ) 
 509               			ADC   R31                , R1                     ;add carry, R1 zero reg (  1 clock ) 
 510               			
 511               			;update flag as task is ready
 512               			LDI   R18                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 513               			ST    Z                  , R18                    ;update flag            (  2 clocks) 
 514               	        
 515               			;find ram address for priority
 516               		    LDI   R30                , lo8(KerSchPr)          ;fetch base pos low     (  1 clock ) 
 517               			LDI   R31                , hi8(KerSchPr)          ;fetch base pos high    (  1 clock ) 
 518               			ADD   R30                , R16                    ;add offset to array    (  1 clock ) 
 519               			ADC   R31                , R1                     ;add carry, R1 zero reg (  1 clock ) 
 520               	        LD    R20                , Z                      ;load priority          (  2 clocks) 
 521               	        CP    R20                , R19                    ;compare                (  1 clock ) 
 522               			BRSH  _LOOP\@                                     ;if R20>=R19, skip save (  2 clocks) 
 523               			MOV   R19                , R20                    ;assign R19 = R20       (  1 clock ) 
 524               			MOV   R21                , R16                    ;high prio task_id      (  1 clock ) 
 525               			
 526               		_LOOP\@:
 527               			;if task_id!=0, start again
 528               			CPI   R16                , 0x00                   ;compare with 0         (  1 clock ) 
 529               			BRNE  _UPDATE_TASK_READY\@                        ;if lc!=0, run again    (  2 clocks) 
 530               	        STS   KerBase+OFB_PTID   , R21                    ;store high prio tak_id (  2 clocks) 
 531               	.endm
