 324               	.macro  KER_LOAD_NEXT_TASK_ID                             ;total 1.50uS @8MHz     ( 12 clocks) 
 325               			MOV   R16                , R22                    ;copy task_id           (  1 clock ) 
 326               			INC   R16                                         ;increment task_id      (  1 clock ) 
 327               			LDS   R23                , KerBase+OFB_NTSK       ;load ntask             (  2 clocks) 
 328               		_MOD_CONT\@:                                          ;use local label
 329               		    CP    R16                , R23                    ;compare ntask & task_id(  1 clock ) 
 330               			BRLO  _MOD_DONE\@                                 ;if R16<R17, done       (  2 clocks) 
 331               			SUB   R16                , R23                    ;subtract R17 from R16  (  1 clock ) 
 332               			RJMP  _MOD_CONT\@                                 ;go to loop start       (  2 clocks) 
 333               		_MOD_DONE\@:                                          ;use local label
 334               		    STS   KerBase+OFB_TID    , R16                    ;save task_id%ntask     (  2 clocks) 
 335               			MOV   R22                , R16                    ;copy task_id           (  1 clock ) 
