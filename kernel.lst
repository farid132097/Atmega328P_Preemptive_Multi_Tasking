 582               	;;============================sleep timeout management starting=============================;; 
 583               	;used registers          : R16, R17, R18, R24, R30, R31, arg R24[0->ISR,1->Thrd], returns R24  
 584               	;unsafe access registers : None                                                                
 585               	.macro  KER_SLP_TIME_MGNT                                 ;total 5.50uS @8MHz     ( 44 clocks) 
 586               			LDI   ZL                 , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 587               			LDI   ZH                 , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 588               			KER_CALC_ADDR_OFF_WORD                            ;calc offset            (  6 clocks) 
 589               			;fetch current value from ram, if val=0, skip decrement                                
 590               	        LDD   R16                , Z+0                    ;load val low byte      (  2 clocks) 
 591               			LDD   R17                , Z+1                    ;load val high byte     (  2 clocks) 
 592               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 593               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 594               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 595               	        CPI   R24                , SCH_MODE_THREAD        ;if arg=1, thread mode  (  1 clock ) 
 596               			BREQ  _VAL_NOT_NULL\@                             ;no need to dec val     (  2 clocks) 
 597               			;R17:R16 contains 16 bit sleep timer val, decrease val by 1                            
 598               			LDI   R18                , 0x01                   ;set val 1              (  1 clock ) 
 599               	        SUB   R16                , R18                    ;subtract low byte      (  1 clock ) 
 600               			LDI   R18                , 0x00                   ;clear                  (  1 clock ) 
 601               			SBC   R17                , R18                    ;subtract carry if any  (  1 clock ) 
 602               			;store new value                                                                       
 603               			STD   Z+0                , R16                    ;store low byte         (  2 clocks) 
 604               			STD   Z+1                , R17                    ;store low byte         (  2 clocks) 
 605               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 606               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 607               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 608               		_VAL_NULL\@:                                                                               
 609               		    ;find ram address for status                                                           
 610               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 611               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 612               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 613               			;update flag as task is ready                                                          
 614               			LDI   R24                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 615               			ST    Z                  , R24                    ;update flag            (  2 clocks) 
 616               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
 617               	    _VAL_NOT_NULL\@:                                                                           
 618               		    LDI   R24                , TASK_BLOCK             ;use as return val      (  1 clock ) 
 619               	    _EXIT_SLP_TIME\@:                                                                          
 620               	.endm                                                                                          
 621               	;;============================sleep timeout management end==================================;; 
