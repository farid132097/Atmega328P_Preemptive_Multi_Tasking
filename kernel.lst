 272               			STS   SRTCNT2            , R18                    ;clear TCNT2            (  2 clocks) 
 273               	        LDI   R18                , 0x02                   ;set TOIE2 bit          (  1 clock ) 
 274               			STS   SRTIMSK2           , R18                    ;set val to TIMSK2      (  2 clocks) 
 275               	.endm                                                                                          
