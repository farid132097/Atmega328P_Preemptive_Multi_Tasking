 692               	.macro  KER_CPU_USAGE                                     ;total 3.25uS @8MHz     ( 26 clocks) 
 693               	        ;check if current target task is idle task or not                                      
 694               	        LDS    R16               , KerBase+OFB_TID        ;load target task_id    (  2 clocks) 
 695               			TST    R16                                        ;check if idle task     (  1 clock ) 
 696               			BREQ   _KER_USG_TICK\@                            ;task_id=idle, skip     (  2 clocks) 
 697               			LDS    R16               , KerBase+OFB_UATC       ;load active tick cnt   (  2 clocks) 
 698               			INC    R16                                        ;inc active tick cnt    (  1 clock ) 
 699               			STS    KerBase+OFB_UATC  , R16                    ;store new val          (  2 clocks) 
 700               		_KER_USG_TICK\@:                                                                           
 701               			LDS    R16               , KerBase+OFB_UTC        ;load usage tick cnt    (  2 clocks) 
 702               			INC    R16                                        ;increment tick cnt     (  1 clock ) 
 703               			CPI    R16               , 100                    ;compare with 100       (  1 clock ) 
 704               			BRLO   _KER_USG_UTC_SV\@                          ;val<100, save new val  (  2 clocks) 
 705               			LDI    R16               , 0x00                   ;val>=100, roll back    (  1 clock ) 
 706               			LDS    R17               , KerBase+OFB_UATC       ;load active tick cnt   (  2 clocks) 
 707               			STS    KerBase+OFB_USAGE , R17                    ;store usage            (  2 clocks) 
 708               			LDI    R17               , 0x00                   ;clear reg              (  1 clock ) 
 709               			STS    KerBase+OFB_UATC  , R17                    ;clear active tick cnt  (  2 clocks) 
 710               		_KER_USG_UTC_SV\@:                                                                         
 711               			STS    KerBase+OFB_UTC   , R16                    ;store new val          (  2 clocks) 
 712               	.endm                                                                                          
