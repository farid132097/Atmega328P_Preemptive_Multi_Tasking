 471               	.macro  KER_DEC_SLP_TIMEOUT_ALL                           ;total 4.88uS @8MHz     ( 39 clocks) 
 472               	        LDS   R16                , KerBase+OFB_NTSK       ;load ntask             (  2 clocks) 
