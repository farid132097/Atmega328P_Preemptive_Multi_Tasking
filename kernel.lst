 475               	;used registers          : R16, R17, R18, R24, R30, R31, arg R24[0->ISR,1->Thrd], returns R24  
 476               	;unsafe access registers : None                                                                
 477               	.macro  KER_DEC_SLP_TIMEOUT                               ;total 4.75uS @8MHz     ( 38 clocks) 
 478               			LDI   ZL                 , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 479               			LDI   ZH                 , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 480               			KER_CALC_ADDR_OFF_WORD                            ;calc offset            (  6 clocks) 
 481               	        
 482               			;fetch current value from ram, if val=0, skip decrement
 483               	        LD    R16                , Z+                     ;load val low byte      (  2 clocks) 
 484               			LD    R17                , Z                      ;load val high byte     (  2 clocks) 
 485               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 486               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 487               			BREQ  _VAL_NULL\@                                 ;val=0, save sts        (  2 clocks) 
 488               	        
 489               	        ;CPI   R24                , 0x01                   ;if arg=1, thrd mode    (  1 clock ) 
 490               			;BREQ  _VAL_NOT_NULL\@                             ;no need to dec val     (  2 clocks) 
 491               	        
 492               			;R17:R16 contains 16 bit sleep timer val, decrease val by 1
 493               			LDI   R18                , 0x01                   ;set val 1              (  1 clock ) 
 494               	        SUB   R16                , R18                    ;subtract low byte      (  1 clock ) 
 495               			LDI   R18                , 0x00                   ;clear                  (  1 clock ) 
 496               			SBC   R17                , R18                    ;subtract carry if any  (  1 clock ) 
 497               	        
 498               			;store new value
 499               			ST    Z                  , R17                    ;store high byte        (  2 clocks) 
 500               			ST    -Z                 , R16                    ;store low byte         (  2 clocks) 
 501               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 502               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 503               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 504               	
 505               		_VAL_NULL\@:
 506               		    ;find ram address for status
 507               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 508               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 509               	        KER_CALC_ADDR_OFF_BYTES                           ;calc offset            (  5 clocks) 
 510               			
 511               			;update flag as task is ready
 512               			LDI   R16                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 513               			ST    Z                  , R16                    ;update flag            (  2 clocks) 
 514               			LDI   R24                , TASK_READY             ;use as return val      (  1 clock ) 
 515               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
 516               	
 517               	    _VAL_NOT_NULL\@:
 518               		    LDI   R24                , TASK_BLOCK             ;use as return val      (  1 clock ) 
 519               	
 520               	    _EXIT_SLP_TIME\@:
