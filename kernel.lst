 560               	;;================================run scheduler starting====================================;; 
 561               	;used registers          : R16, R24, R30, R31, returns R24 as current task priority            
 562               	;unsafe access registers : None       
 563               	.macro  KER_RUN_SCHEDULER                                 ;total 4.88uS @8MHz     ( 39 clocks) 
 564               			LDI    R16                , 0xFF                  ;set 0xff               (  1 clock ) 
 565               			STS    KerBase+OFB_LPR    , R16                   ;lowest priority        (  2 clocks) 
 566               			LDI    R16                , 0x00                  ;start from 0           (  1 clock ) 
 567               			STS    KerBase+OFB_PTID   , R16                   ;highest prio tid=0     (  2 clocks) 
 568               		_LOOP\@:                                                                                   
 569               		    ;store task id to run from KER_DEC_SLP_TIMEOUT                                         
 570               			STS    KerBase+OFB_TID    , R16                   ;store task id          (  2 clocks) 
 571               	        ;sleep time decrement, update ready/blocked status                                     
 572               			LDI    R24                , 0x00                  ;isr mode, dec slp val  (  1 clock ) 
 573               			KER_DEC_SLP_TIMEOUT                               ;update return vars     ( 49 clocks) 
 574               	        CPI    R24                , TASK_READY            ;compare                (  1 clock ) 
 575               	        BRNE   _NEXT_TASK\@                               ;skip if task not ready (  2 clocks) 
 576               	        ;calculate current task priority
 577               			KER_CURR_TASK_PRIO                                ;get task prio ->R24    (  9 clocks) 
 578               	        ;compare current task priority with lowest priority so far                             
 579               			LDS    R16                , KerBase+OFB_LPR       ;load lowest priority   (  2 clocks) 
 580               			CP     R24                , R16                   ;compare                (  1 clock ) 
 581               			BRSH   _NEXT_TASK\@                               ;if prio>=lowest prio   (  2 clocks) 
 582               			;found new lowest priority                                                             
 583               			STS    KerBase+OFB_LPR    , R24                   ;save lowest priority   (  2 clocks) 
 584               			LDS    R16                , KerBase+OFB_TID       ;load task id           (  2 clocks) 
 585               			STS    KerBase+OFB_PTID   , R16                   ;save lowest priority   (  2 clocks) 
 586               	
 587               	    _NEXT_TASK\@:
 588               		    LDS    R16                , KerBase+OFB_TID       ;load task id           (  2 clocks) 
 589               			INC    R16                                        ;increment by 1         (  1 clock ) 
 590               			LDS    R17                , KerBase+OFB_NTSK      ;load ntask             (  2 clocks) 
 591               			CP     R16                , R17                   ;compare with ntask     (  2 clocks) 
 592               			BRLO   _LOOP\@                                    ;if task_id<ntask       (  2 clocks) 
 593               	        LDS    R16                , KerBase+OFB_PTID      ;load high prio task id (  2 clocks) 
 594               			STS    KerBase+OFB_TID    , R16                   ;for test only          (  2 clocks)
 595               	.endm                                                                                          
 596               	;;===================================run scheduler end======================================;; 
