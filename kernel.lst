 545               	.macro  KER_CURR_TASK_PRIO                                ;total 1.13uS @8MHz     (  9 clocks) 
 546               			LDI    ZL                 , lo8(KerSchPr)         ;load low addr          (  1 clock ) 
 547               			LDI    ZH                 , hi8(KerSchPr)         ;load high addr         (  1 clock ) 
 548               			LDI    R16                , 0x00                  ;clear reg, for carry   (  1 clock ) 
 549               			LDS    R24                , KerBase+OFB_TID       ;load task id           (  2 clocks) 
 550               	        ADD    ZL                 , R24                   ;add low addr           (  1 clock ) 
 551               			ADC    ZH                 , R16                   ;add carry if any       (  1 clock ) 
 552               			LD     R24                , Z                     ;load current tid prio  (  2 clocks) 
