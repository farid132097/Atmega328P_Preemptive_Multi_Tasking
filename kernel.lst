 438               	.macro  KER_DEC_SLP_TIMEOUT                               ;total 6.13uS @8MHz     ( 49 clocks) 
 439               			MOV   R16                , R22                    ;copy task_id           (  1 clock ) 
 440               			LSL   R16                                         ;x2                     (  1 clock ) 
 441               			LDI   ZL                 , lo8(KerSchSlp)         ;fetch base pos low     (  1 clock ) 
 442               			LDI   ZH                 , hi8(KerSchSlp)         ;fetch base pos high    (  1 clock ) 
 443               			ADD   ZL                 , R16                    ;add offset to array    (  1 clock ) 
 444               			LDI   R16                , 0x00                   ;clear                  (  1 clock ) 
 445               			ADC   ZH                 , R16                    ;add carry if any       (  1 clock ) 
 446               	
 447               			;fetch current value from ram, if val=0, skip decrement
 448               	        LD    R16                , Z+                     ;load val low byte      (  2 clocks) 
 449               			LD    R17                , Z                      ;load val high byte     (  2 clocks) 
 450               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 451               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 452               			BREQ  _VAL_NULL\@                                 ;if val=0               (  2 clocks) 
 453               	
 454               			;R17:R16 contains 16 bit sleep timer val, decrease val by 1
 455               			LDI   R18                , 0x01                   ;set val 1              (  1 clock ) 
 456               	        SUB   R16                , R18                    ;subtract low byte      (  1 clock ) 
 457               			LDI   R18                , 0x00                   ;clear                  (  1 clock ) 
 458               			SBC   R17                , R18                    ;subtract carry if any  (  1 clock ) 
 459               	        
 460               			;store new value
 461               			ST    Z                  , R17                    ;store high byte        (  2 clocks) 
 462               			ST    -Z                 , R16                    ;store low byte         (  2 clocks) 
 463               			MOV   R18                , R16                    ;copy                   (  1 clock ) 
 464               			OR    R18                , R17                    ;or high & low bytes    (  1 clock ) 
 465               			BRNE  _VAL_NOT_NULL\@                             ;val!=0                 (  2 clocks) 
 466               	
 467               		_VAL_NULL\@:
 468               		    ;find ram address for status
 469               			MOV   R16                , R22                    ;copy task_id           (  1 clock ) 
 470               		    LDI   ZL                 , lo8(KerSchSts)         ;fetch base pos low     (  1 clock ) 
 471               			LDI   ZH                 , hi8(KerSchSts)         ;fetch base pos high    (  1 clock ) 
 472               			ADD   ZL                 , R16                    ;add offset to array    (  1 clock ) 
 473               			LDI   R16                , 0x00                   ;clear                  (  1 clock ) 
 474               			ADC   ZH                 , R16                    ;add carry if any       (  1 clock ) 
 475               			
 476               			;update flag as task is ready
 477               			LDI   R16                , TASK_READY             ;set TASK_READY         (  1 clock ) 
 478               			ST    Z                  , R16                    ;update flag            (  2 clocks) 
 479               			LDI   R24                , TASK_READY             ;use as return val      (  1 clock ) 
 480               	
 481               			;find ram address for status
 482               			MOV   R16                , R22                    ;copy task_id           (  1 clock ) 
 483               		    LDI   ZL                 , lo8(KerSchPr)          ;fetch base pos low     (  1 clock ) 
 484               			LDI   ZH                 , hi8(KerSchPr)          ;fetch base pos high    (  1 clock ) 
 485               			ADD   ZL                 , R16                    ;add offset to array    (  1 clock ) 
 486               			LDI   R16                , 0x00                   ;clear                  (  1 clock ) 
 487               			ADC   ZH                 , R16                    ;add carry if any       (  1 clock ) 
 488               	        LD    R25                , Z                      ;load priority          (  2 clocks) 
 489               			RJMP  _EXIT_SLP_TIME\@                            ;jump to exit           (  2 clocks) 
 490               	
 491               	    _VAL_NOT_NULL\@:
 492               		    LDI   R24                , TASK_BLOCK             ;use as return val      (  1 clock ) 
 493               	        LDI   R25                , 0xFF                   ;use as return val      (  1 clock ) 
 494               	
 495               	    _EXIT_SLP_TIME\@:
 496               	.endm
