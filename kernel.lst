 364               	.macro  KER_TICK_INCREMENT                                ;total 3.38uS @8MHz     ( 27 clocks) 
 365               			LDI   R16                , 0x01                   ;load 1 to R16          (  1 clock ) 
 366               			LDS   R17                , KerBase+OFF_TICK0      ;load Byte0             (  2 clocks) 
 367               			ADD   R17                , R16                    ;add 1 with Byte0       (  1 clock ) 
 368               			STS   KerBase+OFF_TICK0       , R17               ;set Byte0              (  2 clocks) 
 369               			LDI   R16                , 0x00                   ;load 0 to R16          (  1 clock ) 
 370               			LDS   R17                , KerBase+OFF_TICK1      ;load Byte1             (  2 clocks) 
 371               			ADC   R17                , R16                    ;add carry with Byte1   (  1 clock ) 
 372               			STS   KerBase+OFF_TICK1       , R17               ;set Byte1              (  2 clocks) 
 373               			LDS   R17                , KerBase+OFF_TICK2      ;load Byte2             (  2 clocks) 
 374               			ADC   R17                , R16                    ;add carry with Byte2   (  1 clock ) 
 375               			STS   KerBase+OFF_TICK2       , R17               ;set Byte2              (  2 clocks) 
 376               			LDS   R17                , KerBase+OFF_TICK3      ;load Byte3             (  2 clocks) 
 377               			ADC   R17                , R16                    ;add carry with Byte3   (  1 clock ) 
 378               			STS   KerBase+OFF_TICK3       , R17               ;set Byte3              (  2 clocks) 
 379               			LDS   R17                , KerBase+OFF_TICK4      ;load Byte4             (  2 clocks) 
 380               			ADC   R17                , R16                    ;add carry with Byte4   (  1 clock ) 
 381               			STS   KerBase+OFF_TICK4       , R17               ;set Byte4              (  2 clocks) 
 382               	.endm
