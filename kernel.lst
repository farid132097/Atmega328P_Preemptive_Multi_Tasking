 645               	;;================================run scheduler starting====================================;; 
 646               	;used registers          : R16, R24, R25, R30, R31, arg[R24->sch mode], ret none               
 647               	;unsafe access registers : None                                                                
 648               	.macro  KER_RUN_SCHEDULER                                 ;total 11.5uS @8MHz     ( 92 clocks) 
 649               			LDI    R16               , 0xFF                   ;set 0xff               (  1 clock ) 
 650               			STS    KerBase+OFB_LPR   , R16                    ;lowest priority        (  2 clocks) 
 651               			LDI    R16               , 0x00                   ;start from 0           (  1 clock ) 
 652               			STS    KerBase+OFB_PTID  , R16                    ;highest prio tid=0     (  2 clocks) 
 653               			MOV    R25               , R24                    ;copy sch mode          (  1 clock ) 
 654               		_KER_SCH_LOOP\@:                                                                           
 655               		    ;store task id to run from KER_DEC_SLP_TIMEOUT                                         
 656               			STS    KerBase+OFB_TID   , R16                    ;store task id          (  2 clocks) 
 657               	        ;sleep time decrement, update ready/blocked status                                     
 658               			MOV    R24               , R25                    ;restore sch mode       (  1 clock ) 
 659               			KER_SLP_TIME_MGNT                                 ;update return vars     ( 44 clocks) 
 660               	        CPI    R24               , TASK_READY             ;compare                (  1 clock ) 
 661               	        BRNE   _KER_SCH_NEXT\@                            ;skip if task not ready (  2 clocks) 
 662               			KER_CURR_TASK_PRIO                                ;get task prio ->R24    (  9 clocks) 
 663               	        ;compare current task priority with lowest priority found so far                       
 664               			LDS    R16               , KerBase+OFB_LPR        ;load lowest priority   (  2 clocks) 
 665               			CP     R24               , R16                    ;compare                (  1 clock ) 
 666               			BRSH   _KER_SCH_NEXT\@                            ;if prio>=lowest prio   (  2 clocks) 
 667               			;found new lowest priority                                                             
 668               			STS    KerBase+OFB_LPR   , R24                    ;save lowest priority   (  2 clocks) 
 669               			LDS    R16               , KerBase+OFB_TID        ;load task id           (  2 clocks) 
 670               			STS    KerBase+OFB_PTID  , R16                    ;save lowest priority   (  2 clocks) 
 671               	                                                                                               
 672               	    _KER_SCH_NEXT\@:                                                                           
 673               		    LDS    R16               , KerBase+OFB_TID        ;load task id           (  2 clocks) 
 674               			INC    R16                                        ;increment by 1         (  1 clock ) 
 675               			LDS    R17               , KerBase+OFB_NTSK       ;load ntask             (  2 clocks) 
 676               			CP     R16               , R17                    ;compare with ntask     (  2 clocks) 
 677               			BRSH   _KER_SCH_EXIT\@                            ;if task_id>=ntask      (  2 clocks) 
 678               			RJMP   _KER_SCH_LOOP\@                            ;jump to entry          (  2 clocks) 
 679               		_KER_SCH_EXIT\@:                                                                           
 680               	        LDS    R16               , KerBase+OFB_PTID       ;load high prio task id (  2 clocks) 
 681               			STS    KerBase+OFB_TID   , R16                    ;for test only          (  2 clocks) 
 682               	.endm                                                                                          
