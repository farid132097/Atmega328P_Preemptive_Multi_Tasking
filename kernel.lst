 352               			LDI   R21                , 0x01                   ;set reg value 1        (  1 clock ) 
 353               	        ADD   R16                , R21                    ;add 1 with Byte0       (  1 clock ) 
 354               			LDI   R21                , 0x00                   ;clear for carry prop   (  1 clock ) 
 355               	        ADC   R17                , R21                    ;add if carry           (  1 clock ) 
 356               			ADC   R18                , R21                    ;add if carry           (  1 clock ) 
 357               			ADC   R19                , R21                    ;add if carry           (  1 clock ) 
